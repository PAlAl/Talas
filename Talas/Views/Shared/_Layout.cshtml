<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Talas Electric</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    
    <div class="container text-center body-content">
        @RenderBody()
        <footer>
            <div class="text-left navbar-fixed-bottom row-fluid">              
                    <img  src="~/Content/images/talas.png" />
                    <span id="_idContainerFooter" class="CharOverride-16">All rights reserved</span>               
            </div>
        </footer>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
