@using Objects
@model Engine

<div class="row">
    <div class="col-md-4 CharOverride-24 text-left motorName">
        Motor @Model.Name <br />Pulp Dryer, Fan
    </div>
    <div class="col-md-8 ">       
        @{Html.RenderPartial("DatePicker");}
    </div>
</div>

<div class="row" id="content2">
    <div class="col-md-12">
        <table class="tableInfoEngine text-left">
            <tr class="border_bottom">
                <td>Type</td>
                <td>@Model.Type</td>
            </tr>
            <tr class="border_bottom">
                <td>Power</td>
                <td>@Model.Power</td>
            </tr>
            <tr class="border_bottom">
                <td>Voltage</td>
                <td>@Model.Voltage</td>
            </tr>
            <tr class="border_bottom">
                <td>Manufacture date</td>
                <td>@Model.ManufactureDate</td>
            </tr>
            <tr class="border_bottom">
                <td>Installation date</td>
                <td>@Model.InstallationDate</td>
            </tr>
        </table>
    </div>
</div>

@Html.Partial("~/Views/Engine/DownButtons.cshtml")




