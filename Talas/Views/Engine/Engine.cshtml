@using Objects
@model IEnumerable<Engine>

<div class="scrollable">
    <table class="table">
        <caption class="CharOverride-32">Dashboard</caption>
        <thead>
            <tr>
                <th class="text-center">Electric<br />Motors</th>
                <th class="text-center">Date<br />& Time</th>
                <th class="text-center">Resistance<br />kOhm</th>
                <th class="text-center">Motor<br />Operating</th>
                <th class="text-center">Online</th>
            </tr>
        </thead>
        <tbody>
            @foreach (Engine e in Model)
            {
                <tr>
                    <td>
                        <input type="hidden" name="id" value=@e.Id>                      
                        @Ajax.ActionLink(@e.Name, "", new AjaxOptions { UpdateTargetId = "content", Url = "/Engine/Index/"+ @e.Id })
                    </td>
                    <td><span name="Date№@e.Id" class="">00</span></td>
                    <td><span name="Value№@e.Id" class="">00</span></td>
                    <td><span name="Work№@e.Id" class="">00</span></td>
                    <td><span name="Status№@e.Id" class="">00</span></td>
                </tr>
            }
        </tbody>
    </table>
</div>
<div class="scrollable">
    <table class="table">
        <caption class="CharOverride-32">Alerts & Notification</caption>
    </table>
</div>

